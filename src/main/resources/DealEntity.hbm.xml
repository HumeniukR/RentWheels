<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.humeniuk.domain.Deal" table="deal" schema="" catalog="on_wheels_db">
        <id name="id"/>
        <property name="idDeal">
            <column name="id_deal" sql-type="int unsigned" not-null="true"/>
        </property>
        <property name="idRenter">
            <column name="id_renter" sql-type="int unsigned" not-null="true"/>
        </property>
        <property name="message">
            <column name="message" sql-type="varchar" length="200" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>