<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.humeniuk.domain.Category" table="category" schema="" catalog="on_wheels_db">
        <id name="idCategory">
            <column name="id_category" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>