<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.humeniuk.domain.Transport" table="transport" schema="" catalog="on_wheels_db">
        <id name="idTransport">
            <column name="id_transport" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="model">
            <column name="model" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="prise">
            <column name="prise" sql-type="int"/>
        </property>
        <property name="rentTime">
            <column name="rent_time" sql-type="varchar" length="45"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar" length="200"/>
        </property>
        <property name="country">
            <column name="country" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="region">
            <column name="region" sql-type="varchar" length="100"/>
        </property>
        <property name="sity">
            <column name="sity" sql-type="varchar" length="200" not-null="true"/>
        </property>
        <property name="streetName">
            <column name="street_name" sql-type="varchar" length="100"/>
        </property>
        <property name="streetNumber">
            <column name="street_number" sql-type="int"/>
        </property>
        <many-to-one name="categoryByIdCategory" class="com.humeniuk.domain.Category">
            <column name="id_category" not-null="true"/>
        </many-to-one>
        <many-to-one name="personByIdPerson" class="com.humeniuk.domain.Person">
            <column name="id_person" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>